<h1>New user</h1>

<form action="<%= users_url %>" method="post">
  <%= csrf_html %>
  <label for="username">
    username
    <input type="text" name="user[username]">
  </label>

  <label for="password">
    password
    <input type="password" name="user[password]">
  </label>

  <input type="submit" value="Create User">
</form>
